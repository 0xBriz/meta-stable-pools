  "author": "0xBriz",
  "scripts": {
    "start": "npx hardhat node",
    "build": "npx hardhat compile",
    "test": "npx hardhat test",
    "clean": "npx hardhat clean",
    "lint:sol": "solhint -f table contracts/**/*.sol",
    "format": "npx prettier --write 'contracts/**/*.sol'",
    "deploy": "npx hardhat run ./scripts/deploy.ts",
    "deploy:local": "npx hardhat run --network localhost ./scripts/deploy-local.ts",
    "deploy:multicall": "npx hardhat run ./scripts/deploy-multicall.ts",
    "deploy:relayer": "npx hardhat run ./scripts/deploy-relayer.ts",
    "deploy:helper": "npx hardhat run ./scripts/deploy-helper.ts",
    "deploy:goerli": "npx hardhat --network goerli run ./scripts/deploy-goerli.ts"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.3",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-vyper": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "decimal.js": "^10.3.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.9.9",
    "hardhat-gas-reporter": "^1.0.4",
    "keccak": "^3.0.2",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
